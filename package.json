{
	"name": "@jdeighan/create-starter",
	"version": "1.0.3",
	"type": "module",
	"author": "John Deighan",
	"license": "MIT",
	"engines": {
		"node": ">=18.0.0"
	},
	"description": "SvleteKit starter",
	"exports": {
		"./package.json": "./package.json"
	},
	"bin": {
		"create-starter": "npx coffee -c . && node ./src/bin/setup.js",
		"starter-create": "npx coffee -c . && node ./src/bin/setup.js",
		"starter": "npx coffee -c . && node ./src/bin/setup.js",
		"gen-manifest": "npx coffee -c . && node ./src/bin/gen-manifest.js"
	},
	"scripts": {
		"coffee:watch": "npx coffee -c -w .",
		"vite:dev": "vite dev",
		"dev": "npx coffee -c . && run-p coffee:watch vite:dev",
		"build": "npx coffee -c . && cp ./src/service-worker-ORG.js ./src/service-worker.js && vite build",
		"preview": "vite preview"
	},
	"devDependencies": {
		"@jdeighan/base-utils": "^7.0.1",
		"@jdeighan/coffee-utils": "^16.0.9",
		"@jdeighan/svelte-utils": "^2.0.14",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/adapter-vercel": "^3.0.2",
		"@sveltejs/kit": "^1.22.2",
		"autoprefixer": "^10.4.14",
		"coffeescript": "^2.7.0",
		"favicons": "^7.1.3",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.25",
		"postcss-color-function": "^4.1.0",
		"postcss-load-config": "^4.0.1",
		"postcss-preset-env": "^9.0.0",
		"svelte": "^4.0.5",
		"svelte-preprocess": "^5.0.4",
		"vite": "^4.4.3"
	}
}
